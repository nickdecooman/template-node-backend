{
  "parser": "babel-eslint",
  "extends": [
    "plugin:jest/recommended",
    "airbnb-base",
    "airbnb-flow",
    "prettier",
    "prettier/flowtype",
  ],
  "plugins": [
    "flowtype",
    "prettier",
    "jest"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "trailingComma": "all",
      "bracketSpacing": true,
      "singleQuote": true,
    }],
    "no-console": 0
  },
  "env": {
    "jest/globals": true
  }
}
